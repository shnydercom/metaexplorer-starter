{
  "name": "metaexplorer-starter",
  "version": "1.0.0",
  "description": "Project for easily getting started with MetaExplorer",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "services/@my-mxp/*",
    "packages/@my-mxp/*",
    "mods/@my-mxp/*",
    "nocode/@my-mxp/*",
    "apps/@my-mxp/*"
  ],
  "devDependencies": {
    "cross-env": "^7.0.2",
    "lerna": "^3.22.1"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "scripts": {
    "story": "lerna run story --stream",
    "prestart": "lerna run libbuild --stream",
    "firstrun": "yarn preeditor",
    "preeditor": "lerna run initserver && lerna run libbuild --stream",
    "editor": "lerna run editor --stream",
    "start": "lerna run start --stream"
  }
}
